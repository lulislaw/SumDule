<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Dule" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_200</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/black</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_200</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>

        <!-- Customize your theme here. -->
    </style>
    <style name="TextViewInter_regular20">
        <item name="android:fontFamily">@font/inter_regular</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">@color/black</item>
    </style>
    <style name="TextViewInter_regular16">
        <item name="android:fontFamily">@font/inter_regular</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/black</item>
    </style>
    <style name="TextViewInter_regular16white">
        <item name="android:fontFamily">@font/inter_regular</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@color/white</item>
    </style>
    <style name="TextViewInter_bold20">
        <item name="android:fontFamily">@font/inter_bold</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="ExposedDropDownMenu" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox.ExposedDropdownMenu" >
        <item name="boxCornerRadiusBottomEnd">8dp</item>
        <item name="boxCornerRadiusBottomStart">8dp</item>
        <item name="boxCornerRadiusTopEnd">8dp</item>
        <item name="boxCornerRadiusTopStart">8dp</item>
    </style>

    <style name="AutoCompleteTextView" parent="ThemeOverlay.Material3.AutoCompleteTextView" >
        <item name="fontFamily">@font/inter_regular</item>
        <item name="android:fontFamily">@font/inter_regular</item>
        <item name="android:inputType">none</item>
        <item name="maxLines">1</item>
        <item name="android:maxLines">1</item>
    </style>

</resources>